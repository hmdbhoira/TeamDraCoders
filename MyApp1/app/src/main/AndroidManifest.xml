<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.user.myapp1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignupClass"
            android:label="SignUp"
            android:theme="@style/AppTheme"/>
        <activity android:name=".ForgotPasswordClass"
            android:label="Recover Password"
            android:theme="@style/AppTheme" />
        <activity android:name=".LoginClass"
            android:label="Login"
            android:theme="@style/AppTheme"
             />
        <activity android:name=".SignUpPublicClass"
            android:label="Login"
            android:theme="@style/AppTheme"
            />
        <activity android:name=".ButtonRegisterClass"
            android:label="Login"
            android:theme="@style/AppTheme"
            tools:ignore="DuplicateActivity" />
    </application>

</manifest>